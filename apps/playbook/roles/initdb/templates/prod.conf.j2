# 服务类型是公有云远程控制时对应的 Domain
flyway.placeholders.remote_control.public_domain = '{{remote_control_public_domain}}'
# 服务类型是公有云远程控制时，是否启用SSL
flyway.placeholders.remote_control.public_use_ssl = '{{remote_control_public_use_ssl}}'
# 服务类型是私有云远程控制时的连接地址
flyway.placeholders.remote_control.private_addr = '{{remote_control_private_addr}}'
# 服务类型是私有云远程控制时的连接会话信息，逗号隔开的列表格式
flyway.placeholders.remote_control.private_sessions = '{{remote_control_private_sessions}}'


##################################################
# 系统配置
##################################################
# 系统是否初始化标识，True:已进行初始化配置，默认false
flyway.placeholders.system.init = "false"

# 搜索配置
flyway.placeholders.searchable.enable = 'true'
flyway.placeholders.searchable.refresh_enable = 'true'

# 群组搜索配置，依赖搜索配置
flyway.placeholders.searchable.group_enable = 'false'
flyway.placeholders.searchable.group_refresh_enable = 'false'

flyway.placeholders.monitor.slow_threshold=50000000

# 语音识别
flyway.placeholders.rcsr.enable = 'false'
flyway.placeholders.rcsr.config_format = 'pcm'
flyway.placeholders.rcsr.config_rate = '16000'
flyway.placeholders.rcsr.config_bitdepth = '16'
flyway.placeholders.rcsr.config_channel = '1'
flyway.placeholders.rcsr.type = 'xunfei'
flyway.placeholders.rcsr.xunfei.api_url = '{{ rcsr_xunfei_api_url }}'
flyway.placeholders.rcsr.xunfei.appid = '{{ rcsr_xunfei_appid }}'
flyway.placeholders.rcsr.xunfei.apikey = '{{ rcsr_xunfei_apikey }}'

# 通知相关
flyway.placeholders.notice.depart_member_change_enable = 'true'

# 新加入群成员是否允许查看群里是消息
flyway.placeholders.group.permission.gdp_view_chat_history = '1'

# 新加入群成员是否允许查看群里是消息
flyway.placeholders.group.permission.edp_view_chat_history = '1'
flyway.placeholders.setting.ratelimit.enable = 'false'


# 账号锁定，是否清除端上本地数据
flyway.placeholders.conf.lockaccount.clear_local_data_enable = 'false'
# 密码加密
flyway.placeholders.security.password_public_key_rsa
flyway.placeholders.security.password_private_key_rsa
# 资源开关
flyway.placeholders.closable_resources.unsafe_password = 'true'
# 限流流量
flyway.placeholders.rate_limit.default_rate = '100'
flyway.placeholders.rate_limit.sms_rate = '0.2'

