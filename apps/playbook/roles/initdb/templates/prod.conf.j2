flyway.placeholders.conferencecall.vccid='111111'
# 电话会议服务商提供的密码
flyway.placeholders.conferencecall.password='111111'
# 邮件服务器的域名
flyway.placeholders.email.host = '{{ email_host }}'
# 发送邮件的用户名
flyway.placeholders.email.account = '{{ email_account }}'
# 发送邮件的密码
flyway.placeholders.email.password = '{{ email_password }}'
# 发送邮件的邮箱地址
flyway.placeholders.email.sender_email_address = '{{ email_sender_email_address }}'